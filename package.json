{
  "name": "hackathon2022-ossinsight-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api-server": "ts-node --project tsconfig.json cmd/api-server/index.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen:schema-types": "json2ts ./schema/query.schema.json ./types/query.schema.d.ts && json2ts ./schema/panel.schema.json ./types/panel.schema.d.ts && json2ts ./schema/plugin.schema.json ./types/plugin.schema.d.ts",
    "validate:schema": "yarn run validate:query-schema && yarn run validate:panel-schema && yarn run validate:plugin-schema",
    "validate:query-schema": "ajv -s ./schema/query.schema.json -c ajv-formats -d ./configs/*query.json -r ./schema/openapi-2021-09-28.schema.json --strict=false",
    "validate:panel-schema": "ajv -s ./schema/panel.schema.json -c ajv-formats -d ./configs/*panel.json",
    "validate:plugin-schema": "ajv -s ./schema/plugin.schema.json -c ajv-formats -d \"./configs/*plugin.json\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "axios": "^1.1.3",
    "echarts": "^5.4.0",
    "echarts-for-react": "^3.0.2",
    "luxon": "^3.0.4",
    "next": "12.3.1",
    "next-auth": "^4.14.0",
    "node-sql-parser": "^4.5.1",
    "notistack": "^2.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-view": "^1.21.3",
    "serverless-mysql": "^1.5.4",
    "swr": "^1.3.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.0.2",
    "@types/mysql": "^2.15.21",
    "@types/node": "18.11.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^2.1.1",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "json-schema-to-typescript": "^11.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  }
}
