    "@mui/material": "^5.10.10",
    "next": "12.3.1",
    "next-auth": "^4.14.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^2.1.1",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "json-schema-to-typescript": "^11.0.2",
    "typescript": "4.8.4"
  }
